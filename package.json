{
  "name": "dout.dev",
  "version": "0.0.1",
  "description": "The DoUtDev Blog",
  "repository": "git@github.com:pixu1980/dout-dev.git",
  "author": "Pixu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "favicon": "real-favicon generate favicon.config.json favicon.data.json ./src/assets/favicon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "rm -rf .parcel-cache dist node_modules && yarn",
    "cms": "node --experimental-modules ./cms/index.js",
    "prestart": "rm -rf .parcel-cache dist && yarn cms",
    "start": "parcel serve ./src/index.html",
    "prebuild": "rm -rf .parcel-cache dist && yarn cms",
    "build": "parcel build ./src/index.html --no-content-hash --no-source-maps --public-url=https://dout.dev"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.6.0",
    "marked": "^4.1.0",
    "parcel": "^2.7.0",
    "postcss": "^8.4.16",
    "postcss-combine-media-query": "^1.0.1",
    "posthtml": "^0.16.6",
    "posthtml-doctype": "^1.1.1",
    "posthtml-extend": "^0.6.3",
    "posthtml-include": "^1.7.4",
    "posthtml-modules": "^0.9.0",
    "posthtml-prism": "^1.0.4",
    "posthtml-render": "^3.0.0"
  }
}
