{
  "name": "dout.dev",
  "version": "0.0.1",
  "description": "The DoUtDev Blog",
  "repository": "git@github.com:pixu1980/dout-dev.git",
  "author": "Pixu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "favicon": "real-favicon generate favicon.config.json favicon.data.json ./src/assets/favicon",
    "favicon:inject": "real-favicon inject favicon.data.json ./dist ./dist/*.html && real-favicon inject favicon.data.json ./dist/pages ./dist/pages/*.html && real-favicon inject favicon.data.json ./dist/pages/months ./dist/pages/months/*.html && real-favicon inject favicon.data.json ./dist/pages/tags ./dist/pages/tags/*.html && real-favicon inject favicon.data.json ./dist/pages/posts ./dist/pages/posts/*.html",
    "html:minify": "html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true --input-dir ./dist --output-dir ./dist --file-ext html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear:cache": "rm -rf .parcel-cache dist",
    "clear:deps": "rm -rf node_modules",
    "clear": "yarn clear:cache && yarn clear:deps",
    "reset": "yarn clear && yarn",
    "cms": "node --experimental-modules ./cms/index.js",
    "prestart": "yarn clear:cache && yarn cms",
    "start": "parcel serve ./src/index.html",
    "prebuild": "yarn clear:cache && yarn favicon && yarn cms",
    "build": "parcel build ./src/index.html --no-content-hash --no-source-maps --public-url=https://dout.dev",
    "postbuild": "yarn favicon:inject && yarn html:minify"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.3",
    "cli-real-favicon": "0.0.8",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.8.0",
    "marked": "^5.0.1",
    "html-minifier": "^4.0.0",
    "parcel": "^2.8.3",
    "postcss": "^8.4.23",
    "postcss-combine-media-query": "^1.0.1",
    "posthtml": "^0.16.6",
    "posthtml-doctype": "^1.1.1",
    "posthtml-extend": "^0.6.5",
    "posthtml-include": "^1.7.4",
    "posthtml-modules": "^0.9.1",
    "posthtml-render": "^3.0.0"
  }
}
